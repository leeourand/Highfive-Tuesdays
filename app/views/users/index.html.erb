<h2>Listing users</h2>

<table>
  <tr>
    <th>Username</th>
    <th>Email</th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr class="<%= cycle("even", "odd") %>">
    <td><%= user.username %></td>
    <td><%= user.email %></td>
    <td><%= link_to 'Show', user %></td>
  </tr>
<% end %>
</table>
